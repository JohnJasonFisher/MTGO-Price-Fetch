<!--=== Profile ===-->
    <div class="container content profile">
      <div class="row">
        <!--Left Sidebar-->
        <div class="col-md-3 md-margin-bottom-40">
          <img class="img-responsive profile-img margin-bottom-20" src=<%= CardUser.fastest_growing_usercard_card_image_url(current_user.id) %> alt="">

          <!--Datepicker-->

          <!--End Datepicker-->
        </div>
        <!--End Left Sidebar-->

        <!-- Profile Content -->
        <div class="col-md-9">
          <div class="profile-body">
            <!--Service Block v3-->
            <div class="row margin-bottom-10">
              <div class="col-sm-6 sm-margin-bottom-20">
                <div class="service-block-v3 service-block-u">
                  <i class="icon-users"></i>
                  <span class="service-heading">Highest Growing Card You Own</span>
                  <span class="counter"> <%= CardUser.fastest_growing_usercard_card_name(current_user.id) %> </span>

                  <div class="clearfix margin-bottom-10"></div>

                  <div class="row margin-bottom-20">
                    <div class="col-xs-6 service-in">
                      <small>Growth Rate</small>
                      <h4 class="counter"> <%= CardUser.fastest_growing_usercard_growth_rate(current_user.id) %>% </h4>
                    </div>
                  </div>
                  <div class="statistics">
                    <h3 class="heading-xs">Growth <span class="pull-right"><%= CardUser.fastest_growing_usercard_growth_rate(current_user.id) %>%</span></h3>
                    <div class="progress progress-u progress-xxs">
                      <!-- <div style="width: <%= CardUser.fastest_growing_usercard_growth_rate(current_user.id) %>%" aria-valuemax="100" aria-valuemin="0" aria-valuenow="67" role="progressbar" class="progress-bar progress-bar-light"> -->
                      <div style="width: 40%" aria-valuemax="100" aria-valuemin="0" aria-valuenow="67" role="progressbar" class="progress-bar progress-bar-light">

                      </div>
                    </div>
                  </div>
                </div>
              </div>

              <div class="col-sm-6">
                <div class="service-block-v3 service-block-blue">
                  <i class="icon-screen-desktop"></i>
                  <span class="service-heading">Total Collection Value</span>
                  <span class="counter">$<%= CardUser.total_usercard_value(current_user.id) %></span>

                  <div class="clearfix margin-bottom-10"></div>

                  <div class="row margin-bottom-20">
                    <div class="col-xs-6 service-in">
                      <small>Total Quantity of Cards in your Collection</small>
                      <h4 class="counter"><%= CardUser.total_quantity(current_user.id) %></h4>
                    </div>
                  </div>
                  <div class="statistics">
                    <h3 class="heading-xs"> <span class="pull-right">  </span></h3>

                  </div>
                </div>
              </div>
            </div><!--/end row-->
            <!--End Service Block v3-->

            <hr>

            <!--Notification-->
          <div class="funny-boxes funny-boxes-left-orange">
            <div class="panel-heading-v2 overflow-h">
              <h2 class="heading-xs pull-left"><i class="fa fa-bell-o"></i> Notifications</h2>
            </div>
            <ul class="list-unstyled mCustomScrollbar margin-bottom-20" data-mcs-theme="minimal-dark">
            <!-- <li class="notification">
              <i class="icon-custom icon-sm rounded-x icon-bg-yellow icon-line fa fa-bolt"></i>
              <div class="overflow-h">
              </div>
            </li> -->
            </ul>
          </div>
          <!--End Notification-->

          </div>
        </div>
        <!-- End Profile Content -->
      </div>
    </div><!--/container-->

    <!--=== End Profile ===-->
